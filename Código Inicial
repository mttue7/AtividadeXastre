#include <stdio.h>
#include <stdlib.h>

int gera_vetor(){

    int vetor;
    for(int i=0;i<1000;i++)
    {
        vetor=rand()%9999;
        printf(" %d",vetor);
    }

    return vetor;
}

float media(float vetor[]){
  float media, soma = 0;
  int i;

  for(i =0; i<1000; i++){
    soma = soma + vetor[i];
  }

  printf("%.2f", soma/1000);
  return 0;
}

int main()
{
    int vetor;
    vetor=gera_vetor();
    media(vetor);

    return 0;
}
